<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Fragments.ClothesFragment"
    android:background="@color/KalariLabMain">
    <ImageButton
        android:id="@+id/c1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/c2"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/image_button_border"
        app:layout_constraintBottom_toTopOf="@id/c3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        />

    <ImageButton
        android:background="@drawable/image_button_border"

        android:id="@+id/c2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/c4"

        app:layout_constraintStart_toEndOf="@id/c1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        />
    <ImageButton
        android:id="@+id/c3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/c4"
        app:layout_constraintTop_toBottomOf="@id/c1"
        android:background="@drawable/image_button_border"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="0dp"
        android:layout_height="0dp"
        />

    <ImageButton
        android:background="@drawable/image_button_border"

        android:id="@+id/c4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintStart_toEndOf="@id/c3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/c2"

        />


</androidx.constraintlayout.widget.ConstraintLayout>